  "private": true,
  "license": "proprietary",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "prettify": "prettier --write",
    "lint": "yarn lint:js && yarn lint:css",
    "lint:css": "stylelint *.js",
    "lint:eslint:fix": "eslint --ignore-path .eslintignore --fix",
    "lint:eslint": "eslint --ignore-path .eslintignore",
    "lint:js": "yarn lint:eslint -- . ",
    "lint:staged": "lint-staged"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "IE 10"
  ],
  "lint-staged": {
    "*.js": [
      "yarn lint:eslint:fix",
      "git add --force"
    ],
    "*.json": [
      "prettier --write",
      "git add --force"
    ]
  },
  "pre-commit": "lint:staged",
  "dependencies": {
    "base-64": "^0.1.0",
    "knex": "^0.21.2",
    "mysql": "^2.18.1",
    "next": "^9.3",
    "next-iron-session": "^4.1.2",
    "next-seo": "^4.4.0",
    "polished": "^3.5.1",
    "react": "16.13.0",
    "react-dom": "16.13.0",
    "react-icons": "^3.9.0",
    "react-progressive-image": "^0.6.0",
    "react-slick": "^0.27.1",
    "react-table": "^7.2.1",
    "slick-carousel": "^1.8.1",
    "styled-components": "^5.0.1",
    "styled-normalize": "^8.0.7",
    "swr": "^0.2.0"

{
  "name": "sprint-slides-generator",
  "version": "0.1.0",
  "private": true,
  "license": "proprietary",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "migrate": "knex migrate:latest",
    "seed": "knex seed:run",
    "prettify": "prettier --write",
    "lint": "yarn lint:js && yarn lint:css",
    "lint:css": "stylelint *.js",
    "lint:eslint:fix": "eslint --ignore-path .eslintignore --fix",
    "lint:eslint": "eslint --ignore-path .eslintignore",
    "lint:js": "yarn lint:eslint -- . ",
    "lint:staged": "lint-staged",
    "analyze": "cross-env ANALYZE=true yarn build"
  },
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "IE 10"
  ],
  "lint-staged": {
    "*.js": [
      "yarn lint:eslint:fix",
      "git add --force"
    ],
    "*.json": [
      "prettier --write",
      "git add --force"
    ]
  },
  "pre-commit": "lint:staged",
  "dependencies": {
    "@zeit-ui/react": "^1.8.1",
    "@zeit-ui/react-icons": "^1.3.1",
    "base-64": "^1.0.0",
    "date-fns": "^2.16.1",
    "knex": "^0.21.8",
    "mysql": "^2.18.1",
    "next": "^10.0.0",
    "next-iron-session": "^4.1.9",
    "next-seo": "^4.14.0",
    "polished": "^4.0.3",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-icons": "^3.11.0",
    "react-markdown": "^5.0.2",
    "react-progressive-image": "^0.6.0",
    "react-slick": "^0.27.11",
    "react-table": "^7.6.1",
    "slick-carousel": "^1.8.1",
    "slugify": "^1.4.5",
    "styled-components": "^5.2.0",
    "styled-normalize": "^8.0.7",
    "swr": "^0.3.6"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^10.0.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-styled-components": "^1.10.7",
    "cross-env": "^7.0.2",
    "eslint": "^7.12.1",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "lint-staged": "^10.5.0",
    "pre-commit": "^1.2.2",
    "prettier": "^2.1.2",
    "stylelint": "^13.7.2",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0"
  }
}
